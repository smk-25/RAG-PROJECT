def strip_provenance(text: str) -> str:
    """
    Remove source citations, page numbers, chunk references, and other provenance markers from generated text.
    """
    if not text:
        return ""
    
    # Remove specific markers like "(chunk:...)" or "chunk:..."
    text = re.sub(r"\(,?\s*chunk:[^\)]+\)", "", text, flags=re.IGNORECASE)
    text = re.sub(r"chunk:[^\s,]+", "", text, flags=re.IGNORECASE)

    # Remove "(Source: ...)" or "Source: ..."
    text = re.sub(r"\(Source\s*:[^\)]+\)", "", text, flags=re.IGNORECASE)
    text = re.sub(r"Source\s*:[^\s,]+", "", text, flags=re.IGNORECASE)

    # Remove "(, chunk:...)" or similar redundant commas
    text = re.sub(r"\(\s*,\s*", "(", text, flags=re.IGNORECASE)
    text = re.sub(r",\s*,", ",", text, flags=re.IGNORECASE)

    # Remove dangling "of ..." patterns (e.g., "of 223")
    text = re.sub(r"\bof\s+\d+\b", "", text, flags=re.IGNORECASE)

    # Remove empty/invalid parens or brackets
    text = re.sub(r"\(\s*\)", "", text)
    text = re.sub(r"\[\s*\]", "", text)

    # Remove extra whitespace and clean up punctuation
    text = re.sub(r"\s+", " ", text).strip()
    text = re.sub(r"\s+([,\.])", r"\1", text)
    
    return text